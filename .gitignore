# Flutter specific ignores
.dart_tool/
.flutter-plugins
.flutter-plugins-dependencies
.packages
/build/
/web/ # Flutter web output will be in frontend/build/web, but just in case.
*.iml
.metadata
.DS_Store
.env # If any .env files are created at the root

# Node.js / Worker specific ignores
backend/node_modules/
backend/dist/
backend/.wrangler/
backend/npm-debug.log*
backend/yarn-debug.log*
backend/yarn-error.log*
backend/.env # If any .env files are created inside backend

# Monorepo structure specific ignores
frontend/build/
frontend/.dart_tool/
frontend/web/ # Flutter web output will be in frontend/build/web, but just in case.

# IDE specific ignores
.idea/
.vscode/launch.json
.vscode/*.code-workspace

# General ignores
*.log
*.tmp
*.swp
